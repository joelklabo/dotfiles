[user]
        name = Joel Klabo 
        email = joelklabo@gmail.com 
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
        editor = mvim
        mergeoptions = --no-ff
        
[merge]
        tool = vimdiff
[alias]
        co = checkout
